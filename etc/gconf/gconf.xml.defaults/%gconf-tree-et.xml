<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Aktiivsed pluginad">
                                                        <longdesc>Aktiivsete pluginate loetelu. See sisaldab aktiivsete pluginate &quot;asukohti&quot;. Mida see &quot;asukoht&quot; iga plugina jaoks tähendab, seda saab vaadata .gedit-plugin failist.</longdesc>
                                                </local_schema>
                                        </entry>
                                        <dir name="checkupdate">
                                                <entry name="ignore_version">
                                                        <local_schema short_desc="Kuni järgmise versiooni väljastamiseni eiratav versioon">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="filebrowser">
                                                <entry name="filter_pattern">
                                                        <local_schema short_desc="Failisirvija filtrimuster">
                                                                <longdesc>Failisirvija filtri muster. See seda filtrit kasutatakse koos filter_mode võtmes määratud režiimiga.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="filter_mode">
                                                        <local_schema short_desc="Failisirvija filtreerimisrežiim">
                                                                <longdesc>Selle väärtusega määratakse, millised failid sirvijas välja filtreeritakse. Võimalikud väärtused on: none (ei filtreerita midagi), hidden (filtreeritakse peidetud failid), binary (filtreeritakse binaarfailid) ja hidden_and_binary (filtreeritakse nii peidetud kui ka binaarfailid).</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="open_at_first_doc">
                                                        <local_schema short_desc="Asukoha määramine esimese dokumendi järgi">
                                                                <longdesc>Kui märgitud, siis failisirvija plugin avab esimesena kataloogi, kus asub esimene avatud dokument, mida failisirvija pole veel kasutanud. (See leiab kasutust tavaliselt juhul, kui dokument avatakse käsurealt, Nautiluse abil või muudel sarnastel juhtudel)</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <dir name="on_load">
                                                        <entry name="enable_remote">
                                                                <local_schema short_desc="Võrgukohtadelt taastamise lubamine">
                                                                        <longdesc>Kas võrgus asuvatelt kohtadelt taastamine on lubatud või mitte.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="virtual_root">
                                                                <local_schema short_desc="Failisirvija virtuaalne juurkataloog">
                                                                        <longdesc>Failisirvija virtuaalne juurkataloog, mida kasutatakse failisirvija laadimise ajal juhul, kui võtmed onload/tree_view on märgitud. Virtuaalne juurkataloog peab alati kuuluma tegeliku juurkataloogi alla.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="root">
                                                                <local_schema short_desc="Failisirvija juurkataloog">
                                                                        <longdesc>Failisirvija juurkataloog, mida kasutatakse failisirvija laadimise ajal juhul, kui võtmed onload/tree_view on märgitud.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tree_view">
                                                                <local_schema short_desc="Avamine koos puuvaatega">
                                                                        <longdesc>Järjehoidjate vaate asemel puuvaate avamine failisirvija plugina laadimisel</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Süntaksi esiletõstmine lubatud">
                                                                <longdesc>Kas gedit peab lubama süntaksi esiletõstmist.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Menüüs näidatavad kooditabelid">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Automaattuvastatud kodeeringud">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Reanumbrite kirjatüüp printimisel">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Päise kirjatüüp printimisel">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Sisu kirjatüüp printimisel">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Prindi reanumbrid">
                                                                        <longdesc>Kui selle väärtus on 0, siis dokumendile ei lisata printimisel reanumbreid. Vastasel juhul lisab gedit dokumendi printimisel reanumbreid siin määratud sammuga.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Reamurdmine printimisel">
                                                                        <longdesc>Pikkade ridade murdmise määramine printimisel. &quot;GTK_WRAP_NONE&quot; korral murdmist ei toimu, &quot;GTK_WRAP_WORD&quot; korral murtakse sõnavahedest ja &quot;GTK_WRAP_CHAR&quot; korral murtakse murdmispiiril asuvast märgist. NB! Väärtused on tõstutundlikud.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Prindi päis">
                                                                        <longdesc>Kas gedit peab kaasama printimisel ka dokumendi päise.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Süntaksi esiletõstmine printimisel">
                                                                        <longdesc>Kas gedit peab printimisel kasutama süntaksi esiletõstmist.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Suurim hiljutiste failide arv">
                                                                        <longdesc>Suurim hiljuti avatud failide arv, mis kuvatakse &quot;Hiljutiste failide&quot; alammenüüs.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Alumine paneel on nähtav">
                                                                        <longdesc>Kas redaktoriaknast allpool olev alumine paneel peab olema nähtav.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="side_pane">
                                                        <entry name="side_pane_visible">
                                                                <local_schema short_desc="Külgpaan on nähtav">
                                                                        <longdesc>Kas redaktoriaknast vasakul pool olev külgpaan peab olema nähtav.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Olekuriba on nähtav">
                                                                        <longdesc>Kas redaktoriaknast allpool olev olekuriba peab olema nähtav.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Tööriistariba nuppude välimus">
                                                                        <longdesc>Tööriistariba nuppude laad. Võimalikud väärtused on &quot;GEDIT_TOOLBAR_SYSTEM&quot; süsteemi vaikimisi laadi jaoks, &quot;GEDIT_TOOLBAR_ICONS&quot; ainult ikoonide kuvamiseks, &quot;GEDIT_TOOLBAR_ICONS_AND_TEXT&quot; nii ikoonide kui ka teksti kuvamiseks ja &quot;GEDIT_TOOLBAR_ICONS_BOTH_HORIZ&quot; kuvamaks ikoonide kõrval ainult tähtsamat teksti. Väärtused on tõstutundlikud ja seetõttu tuleb jälgida, et need määrataks täpselt nii nagu siin kirjeldatud.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Tööriistariba on nähtav">
                                                                        <longdesc>Kas tööriistariba peab redigeerimisaknas nähtav olema.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Otsingu esiletõstmine lubatud">
                                                                        <longdesc>Kas gedit peab kõiki otsitava teksi vastavusi esile tõstma.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="cursor_position">
                                                        <entry name="restore_cursor_position">
                                                                <local_schema short_desc="Kursori eelmise asukoha taastamine">
                                                                        <longdesc>Kas gedit peab faili laadimisel taastama kursori eelmise asukoha.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="smart_home_end">
                                                        <entry name="smart_home_end">
                                                                <local_schema short_desc="Nutikad Home/End klahvid">
                                                                        <longdesc>Määrab, kuidas liigub kursor HOME ja END klahvide vajutamisel. &quot;DISABLED&quot; puhul liigub kursor rea algusesse või lõppu, &quot;AFTER&quot; puhul liigub kursor esmavajutusel rea algusesse/lõppu, teistkordsel vajutusel aga teksti algusesse/lõppu, eirates seal olevaid tühje ridu ja tühikuid. &quot;BEFORE&quot; puhul liigub esmavajutuse teksti algusesse/lõppu, teistkordsel rea algusesse/lõppu, &quot;ALWAYS&quot; korral liigub aga lihtsalt teksti algusesse/lõppu.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Paremveerise asukoht">
                                                                        <longdesc>Paremveerise asukoha määramine.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Paremveerise kuvamine">
                                                                        <longdesc>Kas gedit peab redigeerimisalas kuvama paremveerist.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Vastavate sulgude esiletõstmine">
                                                                        <longdesc>Kas gedit peab vastavuses olevaid sulge esile tõstma.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Käesolev rida tõstetakse esile">
                                                                        <longdesc>Kas gedit peab käesolevat rida esile tõstma.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Reanumbrite kuvamine">
                                                                        <longdesc>Kas gedit peab redaktoris reanumbreid näitama.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Automaatne taandamine">
                                                                        <longdesc>Kas gedit peab autotaandeid lubama.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Lisa tühikud">
                                                                        <longdesc>Kas gedit peab tabulaatorite asemel lisama tühikuid.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Saki suurus">
                                                                        <longdesc>Tabulaatorimärgi asemel kuvatavate tühikute arvu määramine.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Reamurdmisrežiim">
                                                                        <longdesc>Pikkade ridade murdmise määramine redigeerimisel. &quot;GTK_WRAP_NONE&quot; korral murdmist ei toimu, &quot;GTK_WRAP_WORD&quot; korral murtakse sõnavahedest ja &quot;GTK_WRAP_CHAR&quot; korral murtakse murdmispiiril asuvast märgist. NB! Väärtused on tõstutundlikud.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Suurim tagasivõetavate toimingute arv">
                                                                        <longdesc>Suurim toimingute arv, mida gedit laseb unustada või uuesti täita. Piiramatu arvu toimingute jaoks kasuta väärtust &quot;-1&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="undo_actions_limit">
                                                                <local_schema short_desc="Unustatavate toimingute piirang (AEGUNUD)">
                                                                        <longdesc>Suurim toimingute arv, mida gedit laseb unustada või uuesti täita. Piiramatu arvu toimingute jaoks kasuta väärtust &quot;-1&quot;. Aegunud alates versioonist 2.12.0.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="writable_vfs_schemes">
                                                                <local_schema short_desc="Kirjutatavad VFS-skeemid">
                                                                        <longdesc>Gedit&apos;i poolt kirjutamiseks toetatavate VFS-skeemide loetelu. &apos;file&apos; skeem on kirjutatav vaikimisi.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Automaatse salvestuse intervall">
                                                                        <longdesc>Mitmeminutilise intervalliga salvestab gedit failid automaatselt. See kehtib vaid juhul, kui on sisse lülitatud valik &quot;Automaatne salvestamine&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Automaatne salvestamine">
                                                                        <longdesc>Kas gedit peab faile etteantud ajaintervalliga automaatselt salvestama. Intervalli saad määrata sättega &quot;Automaatse salvestamise intervall&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Varukoopia laiend">
                                                                        <longdesc>Laiend või sufiks, mida kasutatakse failide varukoopiate nimedes. See kehtib vaid juhul kui on sisse lülitatud valik &quot;Tee varukoopiaid&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Varukoopiate loomine">
                                                                        <longdesc>Kas gedit peab faili salvestamisel sellest varukoopia tegema või mitte. Varukoopia faili laiendit saab määrata valikust &quot;Varukoopia laiend&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Laadi skeem">
                                                                        <longdesc>Teksti värvimiseks kasutatava GtkSourceView laadiskeemi ID.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Redaktori kirjatüüp">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Vaikekirjatüüpide kasutamine">
                                                                        <longdesc>Kas redigeeritava teksti jaoks kasutatakse gedit&apos;is määratud kirjatüübi asemel süsteemi vaikimisi kindla laiusega kirjatüüpi. Kui see on märkimata, siis kasutatakse valikus &quot;Redaktori kirjatüüp&quot; määratud kirjatüüpi.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
