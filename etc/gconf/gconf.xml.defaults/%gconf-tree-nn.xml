<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Aktive tilleggsmodular">
                                                        <longdesc>Liste over aktive programtillegg. Lista inneheld «adressa» av dei aktive tillegga. Sjå i fila «.gedit-plugin» for å finna «adressa» til eit gitt tillegg.</longdesc>
                                                </local_schema>
                                        </entry>
                                        <dir name="checkupdate">
                                                <entry name="ignore_version">
                                                        <local_schema short_desc="Versjon å hoppa over til den nye versjonen er utgjeven">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="filebrowser">
                                                <entry name="filter_pattern">
                                                        <local_schema short_desc="Filvisar, filtermønster">
                                                                <longdesc>Filtermønsteret filvisaren skal filtrera etter. Dette filteret virkar oppå filter_mode.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="filter_mode">
                                                        <local_schema short_desc="Filvisar, filtermodus">
                                                                <longdesc>Denne verdien styrer kvar som vert filtrert i filvisaren. Gyldige verdiar er: none (filtrer ingenting), hidden (filtrer gøymde filer), binary (filtrer binære filer) og hidden_and_binary (filtrer både gøymde og binære filer).</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="open_at_first_doc">
                                                        <local_schema short_desc="Sett plassering til første dokument">
                                                                <longdesc>Dersom set til TRUE, vil filvisartillegget visa katalogen til det først opna dokumentet når filvisaren ikkje har vore brukt før. (Så dette gjeld stort sett når eit dokument vert opna frå kommandolinja eller med Nautilus eller tilsvarande.)</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <dir name="on_load">
                                                        <entry name="enable_remote">
                                                                <local_schema short_desc="Tillat gjenopprettingar av eksterne plasseringar">
                                                                        <longdesc>Om eksterne plasseringar skal gjenopprettast.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="virtual_root">
                                                                <local_schema short_desc="Filvisar, virtuell rotkatalog">
                                                                        <longdesc>Virtuell rotkatalog å bruka i filvisaren når filvisaren vert lasta og onload/tree_view er TRUE. Den virtuelle rotkatalogen må alltid vera under den verkelege rota.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="root">
                                                                <local_schema short_desc="Filvisar, rotkatalog">
                                                                        <longdesc>Rotkatalog å bruka i filvisaren når filvisaren vert lasta og onload/tree_view er TRUE.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tree_view">
                                                                <local_schema short_desc="Opna med treframsyning">
                                                                        <longdesc>Opna treframsyninga når filvisaren vert lasta, i staden for bokmerkeframsyninga</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Bruk syntaksmerking">
                                                                <longdesc>Om gedit skal bruka syntaksmerking.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Teiknkodingar viste i menyen">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Automatisk gjenkjente tekstkodingar">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Skrift i linjenummer på utskrifter">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Skrift i topptekst på utskrifter">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Skrift i brødteksten på utskrifter">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Skriv ut linjenummer">
                                                                        <longdesc>Dersom verdien er 0, vert linjenummer ikkje skrivne ut på utskrifter. Elles vert linjenummer skrivne kvar n-te linje, der n er verdien av denne nøkkelen.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Linjebryting på utskrifter">
                                                                        <longdesc>Styrer korleis lange linjer vert brote på utskrifter. Bruk «GTK_WRAP_NONE» dersom du ikkje vil ha bryting. Bruk «GTK_WRAP_WORD» dersom du vil bryta linjene mellom to ord, og «GTK_WRAP_CHAR» for å bryta mellom to vilkårlege teikn. Verdiane må stå nett som her, med store bokstavar.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Skriv ut topptekst">
                                                                        <longdesc>Om gedit skal ta med ein topptekst på utskrifter.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Skriv ut syntaksmerking">
                                                                        <longdesc>Om gedit skal bruka syntaksmerking på utskrifter.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Største tal på nyleg brukte filer">
                                                                        <longdesc>Styrer kva som er det høgste talet på nyleg brukte filer som vert vist i undermenyen «Nyleg brukte filer».</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Botnpanelet er synleg">
                                                                        <longdesc>Om botnpanelet på botnen av redigeringsvindauga skal vera synleg.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="side_pane">
                                                        <entry name="side_pane_visible">
                                                                <local_schema short_desc="Sidepanelet er synleg">
                                                                        <longdesc>Om sidepanelet til venstre for redigeringsvindauget skal vera synleg.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Statuslinja er synleg">
                                                                        <longdesc>Om statuslinja på botnen av redigeringsvindauget skal vera synleg.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Stil på verktøylinjeknappane">
                                                                        <longdesc>Stil på verktøylinjeknappane. Moglege verdiar er: «GEDIT_TOOLBAR_SYSTEM», bruk standardstilen til systemet, «GEDIT_TOOLBAR_ICONS», vis berre ikon, «GEDIT_TOOLBAR_ICONS_AND_TEXT», vis både ikon og tekst, og «GEDIT_TOOLBAR_ICONS_BOTH_HORIZ» for å visa prioritert tekst attmed ikon. Verdiane må vera nett som dei står her, med store bokstavar.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Verktøylinja er synleg">
                                                                        <longdesc>Om verktøylinja skal vera synleg i redigeringsvindauga.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Bruk framheving av søkjetekst">
                                                                        <longdesc>Om gedit skal framheva alle tilfella av søkjeteksten.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="cursor_position">
                                                        <entry name="restore_cursor_position">
                                                                <local_schema short_desc="Gå tilbake til førre markørstilling">
                                                                        <longdesc>Om gedit skal gå tilbake til den førre markørstillinga når ei fil vert lasta.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="smart_home_end">
                                                        <entry name="smart_home_end">
                                                                <local_schema short_desc="Smart Home / End">
                                                                        <longdesc>Styrer korleis markøren flyttar seg når Home- og End-tastane vert trykka. Bruk «DISABLED» for å alltid flytta til starten eller slutten av linja, «AFTER» for å flytta til starten eller slutten av linja første gongen knappane vert trykt og deretter flytta til starten eller slutten av teksten utan å ta omsyn for blankteikn på det påfølgjande trykket, «BEFORE» for å flytta til starten eller slutten av teksten før flytting til starten eller slutten av linja, og «ALWAYS» for å alltid flytte til starten eller slutten av teksten i staden for starten eller slutten av linja.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Plassering av høgremargen">
                                                                        <longdesc>Styrer kor høgremargen står.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Vis høgremarg">
                                                                        <longdesc>Om gedit skal visa høgremargen i redigeringsområdet.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Framhev tilhøyrande parentes">
                                                                        <longdesc>Om gedit skal framheva tilhøyrande parantes.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Framhev aktiv linje">
                                                                        <longdesc>Om gedit skal framheva aktiv linje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Vis linjenummer">
                                                                        <longdesc>Om gedit skal visa linjenummer i redigeringsområdet.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Automatisk innrykk">
                                                                        <longdesc>Om gedit skal bruka automatisk innrykk.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Set inn mellomrom">
                                                                        <longdesc>Om gedit skal setja inn mellomrom i staden for tabulatorar.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Storleik på tabulator">
                                                                        <longdesc>Styrer kor mange mellomrom som vert vist i staden for tabulatorar.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Bryt linjer">
                                                                        <longdesc>Styrer korleis lange linjer vert brote i redigeringsområdet. Bruk «GTK_WRAP_NONE» dersom du ikkje vil ha bryting. Bruk «GTK_WRAP_WORD» dersom du vil bryta linjene mellom to ord, og «GTK_WRAP_CHAR» for å bryta mellom to vilkårlege teikn. Verdiane må stå nett som her, med store bokstavar.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Største tal på angringar">
                                                                        <longdesc>Største tal på handlingar som kan angrast og gjenopprettast. Bruk «-1» for ubegrensa.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="undo_actions_limit">
                                                                <local_schema short_desc="Grense for handlingar som kan angrast (FORELDA)">
                                                                        <longdesc>Største tal på handlingar som kan angrast og gjenopprettast. Bruk «-1» for ubegrensa. Forelda sidan versjon 2.12.0</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="writable_vfs_schemes">
                                                                <local_schema short_desc="Skrivbare VFS-metodar">
                                                                        <longdesc>Liste over VFS-metodar støtta i skrivemodus. Metoden «file» er skrivbar som standard.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Tid mellom automatiske lagringar">
                                                                        <longdesc>Talet på minutt mellom kvar automatisk lagring av endra filer. Dette valet har berre noko å seia dersom valet «Lagra automatisk» er slått på.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Lagra automatisk">
                                                                        <longdesc>Om gedit skal lagra endra filer automatisk etter ei viss tid. Du kan velja tida med valet «Tid mellom automatiske lagringar».</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Ending på tryggleikskopiar">
                                                                        <longdesc>Filending eller vedheng som skal brukast på tryggleikskopiar. Dette valet har berre noko å seia dersom valet «Lag tryggleikskopiar» er slått på.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Lag tryggleikskopiar">
                                                                        <longdesc>Om gedit skal laga tryggleikskopiar av filene han lagar. Du kan velja filendinga på tryggleikskopiane med valet «Ending på tryggleikskopiar».</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Stilskjema">
                                                                        <longdesc>ID-en på eit GtkSourceView-stilskjema brukt til å farga teksten.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Skrift i redigeringsområdet">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Bruk standard skrift">
                                                                        <longdesc>Om standard fastbreiddeskrift til systemet skal brukast i redigeringsområdet. Dersom dette valet er slått av, vert skrifta som er namngjeven i «Skrift i redigeringsområdet » brukt i staden.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
