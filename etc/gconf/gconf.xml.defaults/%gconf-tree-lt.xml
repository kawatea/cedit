<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Aktyvūs įskiepiai">
                                                        <longdesc>Aktyvių įskiepių sąrašas. Jame yra aktyvių įskiepių „Vietos“. Žr. .gedit-plugin failą norėdami rasti nurodyto įskiepio „vietą“.</longdesc>
                                                </local_schema>
                                        </entry>
                                        <dir name="checkupdate">
                                                <entry name="ignore_version">
                                                        <local_schema short_desc="Nepaisoma versija iki kitos versijos išleidimo">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="filebrowser">
                                                <entry name="filter_pattern">
                                                        <local_schema short_desc="Failų naršyklės filtravimo šablonas">
                                                                <longdesc>Filtravimo šablonas, kuriuo filtruoti failų naršyklę. Šis filtras veikia ant filter_mode.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="filter_mode">
                                                        <local_schema short_desc="Failų naršyklės filtravimo režimas">
                                                                <longdesc>Ši reikšmė nurodo kokie failai atfiltruojami failų naršyklėje. Tinkamos reikšmės yra: none (nieko nefiltruoti), hidden (filtruoti paslėptus failus), binary (filtruoti dvejetainius failus) ir hidden_and_binary (filtruoti ir paslėptus, ir dvejetainius failus).</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="open_at_first_doc">
                                                        <local_schema short_desc="Nustatyti vietą į pirmą dokumentą">
                                                                <longdesc>Jei TEIGIAMA, failų naršyklės įskiepis rodys pirmo atverto dokumento aplanką, jei failų naršyklė dar nebuvo naudota. (Taigi, tai paprastai taikoma dokumento atvėrimui iš komandų eilutės arba atveriant jį su nautilus, ir t.t.)</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <dir name="on_load">
                                                        <entry name="enable_remote">
                                                                <local_schema short_desc="Įjungti nutolusių vietų atstatymą">
                                                                        <longdesc>Nurodo ar įjungti nutolusių vietų atstatymą.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="virtual_root">
                                                                <local_schema short_desc="Failų naršyklės virtualus šakninis aplankas">
                                                                        <longdesc>failų naršyklės virtualus šakninis aplankas naudojamas įkeliant failų naršyklės įskiepį ir esant onload/tree_view TEIGIAMOMS reikšmėms. Virtualus šakninis aplankas visada turi būti žemiau tikrojo šakninio.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="root">
                                                                <local_schema short_desc="Failų naršyklės šakninis aplankas">
                                                                        <longdesc>failų naršyklės šakninis aplankas naudojamas įkeliant failų naršyklės įskiepį ir esant onload/tree_view TEIGIAMOMS reikšmėms.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tree_view">
                                                                <local_schema short_desc="Atverti su medžio vaizdu">
                                                                        <longdesc>Atverti medžio rodinį, vietoje žymelių rodinio, kai įkeliamas failų naršyklės įskiepis</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Įjungti sintaksės paryškinimus">
                                                                <longdesc>Ar gedit turėtų įjungti sintaksės paryškinimą.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Koduotės rodomos meniu">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Automatiškai nustatytos koduotės">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Eilučių numerių šriftas spausdinimui">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Antraštės šriftas spausdinimui">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Paprasto teksto šriftas spausdinimui">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Spausdinti eilučių numerius">
                                                                        <longdesc>Jei ši reikšmė yra 0, tai spausdinant nebus įterptas nė vienas eilutės numeris. Kitu atveju gedit spaudins eilučių numerius kas tiek eilučių.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Eilučių laužymo būdas spausdinant">
                                                                        <longdesc>Nurodo kaip laužyti ilgas eilutes spausdinant. Naudokite „GTK_WRAP_NONE“ jei norite, kad eilutės nebūtų laužomos, „GTK_WRAP_WORD“ eilutės laužymui žodžio pabaigoje, ir „GTK_WRAP_CHAR“ eilutės laužymui ties simboliu. Atsiminkite, kad šios reikšmės skiria didžiąsias ir mažąsias raides, todėl pasistenkite, kad jos būtų įrašytos tiksliai taip, kaip parašyta čia.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Spausdinti antraštę">
                                                                        <longdesc>Ar gedit turėtų įtraukti dokumento antraštę, juos spausdinant.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Spausdinti sintaksės paryškinimus">
                                                                        <longdesc>Ar gedit turėtų spausdinti sintaksės paryškinimus, spausdinant dokumentus.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Maksimalus neseniai naudotų failų skaičius">
                                                                        <longdesc>Nurodo maksimalų skaičių neseniai naudotų failų, kurie bus rodomi „Naudoti failai“ submeniu.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Apatinis skydelis yra matomas">
                                                                        <longdesc>Ar turi būti matomas apatinis skydelis redagavimo langų apačioje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="side_pane">
                                                        <entry name="side_pane_visible">
                                                                <local_schema short_desc="Šoninis skydelis yra matomas">
                                                                        <longdesc>Ar turi būti matomas šoninis skydelis redagavimo langų kairėje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Būsenos juosta yra matoma">
                                                                        <longdesc>Ar turi matytis būsenos juosta readagavimo lango apačioje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Įrankių juostos mygtukų stilius">
                                                                        <longdesc>Įrankių juostos mygtukų stilius. Galimos reikšmės yra &quot;GEDIT_TOOLBAR_SYSTEM&quot; norint naudoti esamą sistemos stilių, &quot;GEDIT_TOOLBAR_ICONS&quot; norint matyti tik piktogramas,  &quot;GEDIT_TOOLBAR_ICONS_AND_TEXT&quot; norint matyti ir piktogramas ir tekstą, ir &quot;GEDIT_TOOLBAR_ICONS_BOTH_HORIZ&quot; norint matyti tekstą šalia piktogramų. Pastaba: vertėse raidžių dydžio skirtumai turi reikšmę, taigi įsitikinkit, kad jos atrodo tiksliai taip kaip čia paminėta.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Įrankinė yra matoma">
                                                                        <longdesc>Ar redagavimo languose turi matytis įrankių juosta.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Įjungti paieškos paryškinimus">
                                                                        <longdesc>Ar gedit turėtų paryškinti visus ieškomo teksto pasikartojimus.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="cursor_position">
                                                        <entry name="restore_cursor_position">
                                                                <local_schema short_desc="Perkelti kursorių į ankstesnę poziciją">
                                                                        <longdesc>Ar gedit įkėlus failą turėtų perkelti kursorių į buvusią poziciją.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="smart_home_end">
                                                        <entry name="smart_home_end">
                                                                <local_schema short_desc="Išmanūs Home ir End">
                                                                        <longdesc>Nurodo kaip juda žymeklis, kai paspaudžiami klavišai HOME ir END. „DISABLED“ – visada perkelti į eilutės pradžią / pabaigą, „AFTER“ – perkelti į eilutės pradžią / pabaigą, kai klavišiai paspaudžiami pirmą kartą ir į teksto pradžią / pabaigą nepaisant tuščių tarpų, kai klavišai paspaudžiami antrą kartą, „BEFORE“ – perkelti į teksto pradžią / pabaigą, prieš perkeliant į eilutės pradžią / pabaigą, ir „ALWAYS“ – užuot perkėlus į eilutės pradžią / pabaigą, visada perkelti į teksto pradžią / pabaigą.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Dešinės paraštės pozicija">
                                                                        <longdesc>Nurodo dešinės paraštės poziciją.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Rodyti dešinę paraštę">
                                                                        <longdesc>Ar gedit turėtų rodyti dešinę paraštę redagavimo lange.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Paryškinti atitinkantį skliaustelį">
                                                                        <longdesc>Ar gedit turėtų paryškinti skliaustą, porinį pažymėtajam.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Paryškinti esamą eilutę">
                                                                        <longdesc>Ar gedit turėtų paryškinti esamą eilutę.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Rodyti eilučių numerius">
                                                                        <longdesc>Ar gedit turėtų rodyti eilučių numerius redagavimo lange.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Automatinis atitraukimas">
                                                                        <longdesc>Ar gedit turėtų įjungti automatinį atitraukimą.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Įterpti tarpus">
                                                                        <longdesc>Ar gedit turėtų įterpti tarpus vietoj tabuliacijos.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Tabuliacijos dydis">
                                                                        <longdesc>Nurodo, kiek tarpų turėtų būti rodoma vietoj tabuliavimo simbolio.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Eilučių laužymo būdas">
                                                                        <longdesc>Nurodo kaip laužyti eiluters redagavimo vietoje. Naudokite &quot;GTK_WRAP_NONE&quot; jei nenorite, kad eilutės būtų laužomos,  &quot;GTK_WRAP_WORD&quot; eilučių laužymui žodžio pabaigoje ir &quot;GTK_WRAP_CHAR&quot; eilučių laužymui ties simboliu. Atsiminkite, kad šios reikšmės skiria didžiąsias ir mažąsias reikšmes, todėl pasistenkite, kad jos būtų įrašytos tiksliai tokios kokios yra čia.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Maksimalus atšaukiamų veiksmų skaičius">
                                                                        <longdesc>Maksimalus veiksmų skaičius, kuriuos gedit galės atstatyti arba daryti iš naujo. Naudoti „-1“ neribotam veiksmų skaičiui.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="undo_actions_limit">
                                                                <local_schema short_desc="Atšaukiamų veiksmų limitas (NEBENAUDOJAMA)">
                                                                        <longdesc>Maksimalus veiksmų skaičius, kuriuos gedit galės atstatyti arba daryti iš naujo. Naudoti „-1“ neribotam veiksmų skaičiui. Griežtai smerkiama nuo  2.12.0</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="writable_vfs_schemes">
                                                                <local_schema short_desc="Rašomos VFS schemos">
                                                                        <longdesc>VFS schemų, kuriose gedit gali rašyti, sąrašas. Schemoje „file“ rašymas numatytas iš karto.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Automatinio išsaugojimo intervalas">
                                                                        <longdesc>Minučių skaičius, kurį gedit lauks, kad išsaugotų pakeistus failus. Galios tik tada, jei parinktis „Automatinis išsaugojimas“ yra įjungta.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Automatinis saugojimas">
                                                                        <longdesc>Ar gedit turėtų periodiškai išsaugoti pakeistus failus. Periodą galima nustatyti &quot;Autosave Interval&quot; parinktyje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Atsarginės failo kopijos priesaga">
                                                                        <longdesc>Atsarginių failų pavadinimų prievardis. Tai turės reikšmės tik tada, jei parinktis „Kurti atsargines kopijas“ yra įjungta.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Kurti atsargines kopijas">
                                                                        <longdesc>Ar gedit turėtų kurti atsargines failų kopijas jas saugant. Atsarginės failo kopijos priesagą galima nustatyti naudojant &quot;Atsarginės kopijos priesaga&quot; parinktį.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Stiliaus schema">
                                                                        <longdesc>GtkSourceView stiliaus schemos, naudojamos spalvinti tekstui, ID.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Redaktoriaus šriftas">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Naudoti numatytąjį šriftą">
                                                                        <longdesc>Ar naudoti sistemos derintą šriftą vietoje skirto vien tik gedit. Jeigu ši parinktis išjungta, tai vietoje sistemos naudojamo šrifto bus naudojamas šriftas, pasirinktas „Redaktoriaus šriftas“ parinktyje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
