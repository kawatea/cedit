<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Dejavni vstavki">
                                                        <longdesc>Seznam dejavnih vstavkov. Vsebuje &quot;Mesto&quot; dejavnih vstavkov. Glejte datoteko .gedit-plugin za pridobitev &quot;Mesta&quot; danega vstavka.</longdesc>
                                                </local_schema>
                                        </entry>
                                        <dir name="checkupdate">
                                                <entry name="ignore_version">
                                                        <local_schema short_desc="Prezrta različica do objave naslednje različice">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="drawspaces">
                                                <entry name="draw_trailing">
                                                        <local_schema short_desc="Izris presledkov na koncu vrstic">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_text">
                                                        <local_schema short_desc="Izris presledkov v besedilu">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_leading">
                                                        <local_schema short_desc="Izris začetnih presledkov">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_nbsp">
                                                        <local_schema short_desc="Izriši nedeljive presledke">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_newline">
                                                        <local_schema short_desc="Izris znakov za nove vrstice">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_spaces">
                                                        <local_schema short_desc="Izris presledkov">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_tabs">
                                                        <local_schema short_desc="Izris tabulatorjev">
                                                        </local_schema>
                                                </entry>
                                                <entry name="enable">
                                                        <local_schema short_desc="Pokaži presledne znake">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="showtabbar">
                                                <entry name="tabbar_visible">
                                                        <local_schema short_desc="Vrstica zavihkov je vidna">
                                                                <longdesc>Ali naj bo vrstica zavihkov vidna v urejevalnih oknih.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="filebrowser">
                                                <entry name="filter_pattern">
                                                        <local_schema short_desc="Filtrirni vzorec pregledovalnika datotek">
                                                                <longdesc>Filtrirni vzorec za filtriranje pregledovalnika datotek. Ta filter deluje nad filter_mode.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="filter_mode">
                                                        <local_schema short_desc="Filtrirni način pregledovalnika datotek">
                                                                <longdesc>Ta vrednost določa, katere datoteke so filtrirane v pregledovalniku datotek. Veljavne vrednosti so: none (ne filtriraj), hidden (filtriraj skrite datoteke), binary (filtriraj binarne datoteke) in hidden_and_binary (filtriraj tako skrite kot binarne datoteke).</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="open_at_first_doc">
                                                        <local_schema short_desc="Nastavi mesto na prvi dokument">
                                                                <longdesc>Izbrana možnost omogoča, da bo vstavek pregledovalnika datotek prikazal imenik prvega odprtega dokumenta, če pregledovalnik še ni bil uporabljen. (Čeprav se ponavadi to nanaša na odpiranje dokumenta iz ukazne vrstice ali na odpiranje s programom nautilus itd.)</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <dir name="on_load">
                                                        <entry name="enable_remote">
                                                                <local_schema short_desc="Omogoči povrnitev oddaljenih mest">
                                                                        <longdesc>Nastavi, ali je omogočena povrnitev oddaljenih mest.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="virtual_root">
                                                                <local_schema short_desc="Navidezni korenski imenik pregledovalnika datotek">
                                                                        <longdesc>Navidezni korenski imenik pregledovalnika datotek, ki naj se uporabi pri nalaganju vstavka pregledovalnika datotek in ko je onload/tree_view RESNIČNO (TRUE). Navidezni koren mora vedno biti pod dejanskim korenom.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="root">
                                                                <local_schema short_desc="Korenski imenik pregledovalnika datotek">
                                                                        <longdesc>Korenski imenik pregledovalnika datotek, ki naj se uporabi pri nalaganju vstavka pregledovalnika datotek in ko je onload/tree_view RESNIČNO (TRUE).</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tree_view">
                                                                <local_schema short_desc="Odpri z drevesnim prikazom">
                                                                        <longdesc>Odpri drevesni prikaz namesto prikaza zaznamkov, ko se zažene vstavek pregledovalnika datotek</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Omogoči poudarjanje skladnje">
                                                                <longdesc>Ali naj gedit omogoči poudarjanje skladnje.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Kodiranja prikazana v meniju">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Samodejno zaznan nabor z nakov">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Pisava številk vrstic za tiskanje">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Pisava glave za tiskanje">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Pisava telesa za tiskanje">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Natisni številke vrstic">
                                                                        <longdesc>V primeru, da je vrednost 0, potem številke vrstic ob tiskanju dokumenta ne bodo izpisane. V nasprotnem primeru bo Gedit vsakih toliko vrstic izpisoval številke vrstic.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Tiskanje v načinu preloma vrstic">
                                                                        <longdesc>Določa kako naj se prelomijo dolge vrstice pri tiskanju. &quot;GTK_WRAP_NONE&quot; za brez lomljenja, &quot;GTK_WRAP_WORD&quot; za lomljenje po besedah in &quot;GTK_WRAP_CHAR&quot; za lomljenje po posameznih znakih. Te vrednosti so občutljive na velikost črk, zato bodite pazljivi.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Natisni glave strani">
                                                                        <longdesc>Ali naj gedit vključi glavo dokumenta pri tiskanju.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Natisni poudarjanje skladnje">
                                                                        <longdesc>Ali naj gedit poudari skladnjo dokumentov med tiskanjem.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Največ nedavnih datotek">
                                                                        <longdesc>Določa največje število nedavno odprtih datotek, ki bodo prikazane v podmeniju &quot;Nedavne datoteke&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Spodnji pladenj je viden">
                                                                        <longdesc>Ali naj bo spodnji pladenj pod urejevalnim oknom viden.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="side_pane">
                                                        <entry name="side_pane_visible">
                                                                <local_schema short_desc="Stranski pladenj je viden">
                                                                        <longdesc>Ali naj bo stranski pladenj na levi strani urejevalnega okna viden.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Vrstica stanja je vidna">
                                                                        <longdesc>Ali naj bo vrstica stanja na dnu urejevalnega okna vidna.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Slog gumbov orodne vrstice">
                                                                        <longdesc>Slog gumbov orodne vrstice. Mogoče vrednosti so &quot;GEDIT_TOOLBAR_SYSTEM&quot;, ki uporabi privzet sistemski slog, &quot;GEDIT_TOOLBAR_ICONS&quot;, da se prikažejo le ikone, &quot;GEDIT_TOOLBAR_ICONS_AND_TEXT&quot;, da se prikažejo tako ikone kot besedilo in &quot;GEDIT_TOOLBAR_ICONS_BOTH_HORIZ&quot;, da se prikaže pomembnejše besedilo ob ikonah. Te vrednosti so občutljive na velikost črk, zato bodite pazljivi.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Orodna vrstica je vidna">
                                                                        <longdesc>Ali naj bo orodna vrstica vidna v urejevalnih oknih.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Omogoči poudarjanje zadetkov">
                                                                        <longdesc>Ali naj gedit poudari pojavljanje iskanega besedila.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="cursor_position">
                                                        <entry name="restore_cursor_position">
                                                                <local_schema short_desc="Povrni prejšnji položaj kazalca">
                                                                        <longdesc>Ali naj gedit povrne prejšnji položaj kazalca, ko se datoteka naloži.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="smart_home_end">
                                                        <entry name="smart_home_end">
                                                                <local_schema short_desc="Pametni tipki HOME in END">
                                                                        <longdesc>Določa kako se kazalnik premika kadar sta pritisnjeni tipki HOME in END. Uporabite &quot;ONEMOGOČENO&quot; da se vedno premakne med začetkom in koncem vrstice, možnost &quot;PO&quot; za premikanje na začetek in konec ob prvem pritisku tipk in na začetek in konec besedila brez upoštevanja preslednih znakov ob naslednjem pritisku, Možnost &quot;PRED&quot; za premik na začetek in konec besedila pred premikanjem na začetek in konec vrstice in možnost &quot;VEDNO&quot; za premik na začetek in konec besedila namesto na začetek in konec vrstice.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Položaj desnega odmika">
                                                                        <longdesc>Določa položaj desnega robu</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Pokaži desni rob">
                                                                        <longdesc>Ali naj gedit v urejevalni površini pokaže desni odmik.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Poudari pripadajoče oklepaje">
                                                                        <longdesc>Ali naj gedit poudari oklepaj, ki pripada dvojici izbranega oklepaja.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Poudari trenutno vrstico">
                                                                        <longdesc>Ali naj gedit poudari trenutno vrstico.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Pokaži številke vrstic">
                                                                        <longdesc>Ali naj gedit pokaže številke vrstic v urejevalni površini.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Samodejno zamikanje">
                                                                        <longdesc>Ali naj gedit omogoči samodejno zamikanje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Vstavi presledke">
                                                                        <longdesc>Ali naj gedit vstavlja presledke namesto zamikov (tabulatorjev).</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Velikost zamika">
                                                                        <longdesc>Določa število presledkov, ki naj bodo prikazani namesto znaka za zamik (tab).</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Način preloma vrstic">
                                                                        <longdesc>Določa kako naj se prelomijo dolge vrstice v urejevalni površini. &quot;GTK_WRAP_NONE&quot; za brez lomljenja, &quot;GTK_WRAP_WORD&quot; za lomljenje po besedah in &quot;GTK_WRAP_CHAR&quot; za lomljenje po posameznih znakih. Te vrednosti so občutljive na velikost črk, zato bodite pazljivi.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Največje število razveljavitev">
                                                                        <longdesc>Največje število dejanj, ki jih gedit lahko razveljavi ali obnovi. Uporabite &quot;-1&quot; za neomejeno število dejanj.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="undo_actions_limit">
                                                                <local_schema short_desc="Omejitev števila razveljavljenih dejanj (NI VEČ V UPORABI)">
                                                                        <longdesc>Največje število dejanj, ki jih gedit lahko razveljavi ali obnovi. Uporabite &quot;-1&quot; za neomejeno število dejanj. Ni v uporabi od 2.12.0</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="writable_vfs_schemes">
                                                                <local_schema short_desc="Zapisljive sheme VFS">
                                                                        <longdesc>Seznam shem VFS, ki jih gedit podpira v pisnem načinu. Shema &apos;datoteka (file)&apos; je privzeto zapisljiva.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Časovni razmik samodejnega shranjevanja">
                                                                        <longdesc>Število minut po katerih bo gedit samodejno shranil spremenjene datoteke. Upoštevano bo le v primeru, da je možnost &quot;Samodejno shranjevanje&quot; omogočena.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Samodejno shranjevanje">
                                                                        <longdesc>Ali naj gedit samodejno shrani spremenjene datoteke po določenem času. Čas lahko nastavite z možnostjo &quot;Časovni razmik samodejnega shranjevanja&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Pripona varnostnih kopij datotek">
                                                                        <longdesc>Pripona oziroma končnica varnostnih kopij datotek. Upoštevana bo le v primeru, da je možnost &quot;Ustvarjaj varnostne kopije&quot; omogočena.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Ustvarjaj varnostne kopije">
                                                                        <longdesc>Ali naj gedit ustvari varnostne kopije datotek, ki jih shrani. Pripono varnostnih datotek lahko nastavite z možnostjo &quot;Pripona varnostnih kopij datotek&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Oblika sheme">
                                                                        <longdesc>ID GtkSourceView oblike sheme uporabljen za obarvanje besedila.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Pisava urejevalnika">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Uporabi privzeto pisavo">
                                                                        <longdesc>Ali naj se uporabi privzeta sistemska pisava s stalno širino za urejanje besedila namesto pisave uporabljene v programu Gedit. V primeru, da ta možnost ni izbrana, bo namesto sistemske pisave uporabljena tista pisava, ki je imenovana kot možnost &quot;Pisava urejevalnika&quot;.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
