<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Aktivní zásuvné moduly">
                                                        <longdesc>Seznam aktivních zásuvných modulů. Obsahuje „Umístění“ aktivních modulů. Pro získání „Umístění“ daného modulu viz soubor .gedit-plugin.</longdesc>
                                                </local_schema>
                                        </entry>
                                        <dir name="checkupdate">
                                                <entry name="ignore_version">
                                                        <local_schema short_desc="Verze, která má být do vydání další verze ignorována">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="drawspaces">
                                                <entry name="draw_trailing">
                                                        <local_schema short_desc="Vykreslovat koncové mezery">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_text">
                                                        <local_schema short_desc="Vykreslovat mezery v textu">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_leading">
                                                        <local_schema short_desc="Vykreslovat úvodní mezery">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_nbsp">
                                                        <local_schema short_desc="Vykreslovat nezlomitelné mezery">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_newline">
                                                        <local_schema short_desc="Vykreslovat zakončení řádků">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_spaces">
                                                        <local_schema short_desc="Vykreslovat mezery">
                                                        </local_schema>
                                                </entry>
                                                <entry name="draw_tabs">
                                                        <local_schema short_desc="Vykreslovat tabulátory">
                                                        </local_schema>
                                                </entry>
                                                <entry name="enable">
                                                        <local_schema short_desc="Zobrazovat bílé znaky">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="showtabbar">
                                                <entry name="tabbar_visible">
                                                        <local_schema short_desc="Lišta karet je viditelná">
                                                                <longdesc>Zda má být lišta karet viditelná v editačních oknech.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="filebrowser">
                                                <entry name="filter_pattern">
                                                        <local_schema short_desc="Filtrační vzorek prohlížeče souborů">
                                                                <longdesc>Filtrační vzorek, který se používá pro filtrování v prohlížeči souborů. Tento filtr pracuje navrchu filter_mode.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="filter_mode">
                                                        <local_schema short_desc="Filtrační režim prohlížeče souborů">
                                                                <longdesc>Tato hodnota určuje, které soubory budou vyfiltrovány z prohlížeče souborů. Platné hodnoty jsou: none (nic nevyfiltruje), hidden (vyfiltruje skryté soubory), binary (vyfiltruje binární soubory) a hidden_and_binary (vyfiltruje jak skryté, tak binární soubory).</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="open_at_first_doc">
                                                        <local_schema short_desc="Nastavit umístění na první dokument">
                                                                <longdesc>Pokud je nastavené na TRUE, prohlížeč souborů zobrazí adresář prvního otevřeného dokumentu, jestliže nebyl prohlížeč souborů dosud použit. (Tedy toto obecně platí pro otevírání dokumentů z příkazové řádky, nebo v Nautilu atd.)</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <dir name="on_load">
                                                        <entry name="enable_remote">
                                                                <local_schema short_desc="Povolit obnovu vzdálených umístění">
                                                                        <longdesc>Nastavuje, zda je povolena obnova vzdálených umístění.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="virtual_root">
                                                                <local_schema short_desc="Virtuální kořenový adresář prohlížeče souborů">
                                                                        <longdesc>Virtuální kořenový adresář prohlížeče souborů, který bude použit během načítání zásuvného modulu prohlížeče souborů, když onload/tree_view je TRUE. Virtuální kořen musí být vždy pod skutečným kořenem.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="root">
                                                                <local_schema short_desc="Kořenový adresář prohlížeče souborů">
                                                                        <longdesc>Kořenový adresář prohlížeče souborů, který bude použit, když načítání zásuvného modulu prohlížeče souborů a onload/tree_view je TRUE.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tree_view">
                                                                <local_schema short_desc="Otevřít ve stromovém pohledu">
                                                                        <longdesc>Otevřít stromové zobrazení, když se zásuvný modul prohlížeče souborů načte místo zobrazení záložek</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Povolit zvýrazňování syntaxe">
                                                                <longdesc>Jestli má gedit povolit zvýrazňování syntaxe.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Kódování zobrazovaná v nabídce">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Automaticky zjistit kódování">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Písmo číslování řádků pro tisk">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Písmo záhlaví pro tisk">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Písmo těla textu pro tisk">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Tisknout čísla řádků">
                                                                        <longdesc>Je-li hodnota nastavena na 0, nebudou tištěna žádná čísla řádků. Jinak bude vytištěno číslo řádku vždy po tomto počtu řádků.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Zalamovat řádky při tisku">
                                                                        <longdesc>Udává, jak při tisku zalamovat řádky. Hodnota „GTK_WRAP_NONE“ řádky zalamovat nebude, hodnota „GTK_WRAP_WORD“ bude zalamovat řádky na hranicích slov, a hodnota „GTK_WRAP_CHAR“ bude zalamovat na hranicích znaků. Tyto hodnoty jsou citlivé na malá a velká písmena, takže dejte pozor, aby odpovídaly přesně hodnotám, které jsou zde popsány.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Tisknout záhlaví">
                                                                        <longdesc>Má-li být vložena hlavička dokumentu při jeho tisku.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Tisknout zvýraznění syntaxe">
                                                                        <longdesc>Jestli má gedit tisknout zvýraznění syntaxe při tisku dokumentů.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Maximální počet nedávno otevřených souborů">
                                                                        <longdesc>Udává maximální počet nedávno otevřených souborů, které se budou zobrazovat v podnabídce „Otevřít nedávný“.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Spodní panel je viditelný">
                                                                        <longdesc>Má-li být viditelný spodní panel pod editační oblastí.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="side_pane">
                                                        <entry name="side_pane_visible">
                                                                <local_schema short_desc="Postranní panel je zobrazen">
                                                                        <longdesc>Zda má být viditelný postranní panel nalevo od editačního okna.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Stavový panel je viditelný">
                                                                        <longdesc>Má-li být viditelná stavová lišta pod editační oblastí.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Styl tlačítek panelu nástrojů">
                                                                        <longdesc>Styl pro tlačítka lišty nástrojů. Možné hodnoty jsou „GEDIT_TOOLBAR_SYSTEM“, která bude používat standardní styl nastavený v systému, „GEDIT_TOOLBAR_ICONS“, která bude zobrazovat pouze ikony, „GEDIT_TOOLBAR_ICONS_AND_TEXT“, která bude zobrazovat ikony i text, a „GEDIT_TOOLBAR_ICONS_BOTH_HORIZ“, která bude zobrazovat prioritní text vedle ikon. Tyto hodnoty jsou citlivé na malá a velká písmena, takže dejte pozor, aby odpovídaly přesně hodnotám, které jsou zde popsány.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Panel nástrojů je viditelný">
                                                                        <longdesc>Má-li gedit zobrazovat v editačních oknech stavový panel.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Povolit zvýrazňování vyhledaného">
                                                                        <longdesc>Jestli má gedit zvýrazňovat všechny výskyty vyhledaného textu.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="cursor_position">
                                                        <entry name="restore_cursor_position">
                                                                <local_schema short_desc="Obnovit předchozí umístění kurzoru">
                                                                        <longdesc>Má-li gedit po otevření souboru zobrazit kurzor v stejném místě jako před uzavřením souboru.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="smart_home_end">
                                                        <entry name="smart_home_end">
                                                                <local_schema short_desc="Chytré Home/End">
                                                                        <longdesc>Udává, jak se kurzor bude pohybovat, když jsou stisknuty klávesy Domů a Konec. Použijte „Zakázáno“, pokud se chcete vždy přesunout na začátek/konec řádku, „Po“, pokud se chcete při prvním tisknutí přesunout na začátek/konec řádku a po druhém stisknutí přesunout na začátek/konec textu ignorujíce mezery, „Před“, pokud se chcete přesunout na začátek/konec textu před přesunutím na začátek/konec řádku a „Vždy“, pokud se chcete vždy přesunout na začátek/konec textu místo na začátek/konec řádku.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Pozice pravého okraje">
                                                                        <longdesc>Určuje pozici pravého okraje.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Zobrazovat pravý okraj">
                                                                        <longdesc>Jestli má gedit zobrazovat v editační oblasti pravý okraj.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Zvýrazňovat odpovídající závorku">
                                                                        <longdesc>Zda má gedit zvýrazňovat závorky, které si navzájem odpovídají.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Zvýrazňovat aktuální řádek">
                                                                        <longdesc>Jestli má gedit zvýrazňovat aktuální řádek.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Zobrazovat čísla řádků">
                                                                        <longdesc>Jestli má gedit zobrazovat čísla řádků v editační oblasti.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Automatické odsazování">
                                                                        <longdesc>Zda má gedit povolit automatické odsazování.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Vložit mezery">
                                                                        <longdesc>Jestli má gedit vkládat mezery místo tabelátorů.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Velikost tabelátoru">
                                                                        <longdesc>Udává počet mezerníků, které by se měly zobrazovat místo znaků tabelátoru.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Zalamování řádků">
                                                                        <longdesc>Udává, jak zalamovat řádky v editační oblasti. Hodnota „GTK_WRAP_NONE“ řádky zalamovat nebude, hodnota „GTK_WRAP_WORD“ bude zalamovat řádky na hranicích slov, a hodnota „GTK_WRAP_CHAR“ bude zalamovat na hranicích znaků. Tyto hodnoty jsou citlivé na malá a velká písmena, takže dejte pozor, aby odpovídaly přesně hodnotám, které jsou zde popsány.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Max. počet akcí, které lze vrátit zpět">
                                                                        <longdesc>Maximální počet akcí, které bude gedit moci vzít zpět nebo provést znovu. Použijte „-1“, chcete-li neomezený počet akcí.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="undo_actions_limit">
                                                                <local_schema short_desc="Limit pro zpětné akce (ZASTARALÉ)">
                                                                        <longdesc>Maximální počet akcí, které bude gedit moci vzít zpět nebo provést znovu. Použijte „-1“, chcete-li neomezený počet akcí. Zastaralé od 2.12.0</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="writable_vfs_schemes">
                                                                <local_schema short_desc="Zapisovatelná schémata VFS">
                                                                        <longdesc>Seznam schémat VFS, která gedit podporuje v režimu zápisu. Do schématu „file“ lze zapisovat standardně.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Interval pro automatické ukládání">
                                                                        <longdesc>Čas v minutách, po kterém gedit automaticky uloží změněné soubory. Toto nastavení bude mít vliv pouze v případě, že je zapnuta volba „Automatické ukládání“.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Automaticky ukládat">
                                                                        <longdesc>Zda má gedit automaticky ukládat změněné soubory po stanoveném časovém intervalu. Časový interval lze nastavit volbou „Interval pro automatické ukládání“.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Přípona záložní kopie">
                                                                        <longdesc>Přípona, která bude použita pro jména souborů záložních kopií. Toto nastavení bude mít vliv pouze v případě, že je zapnuta volba „Vytvářet záložní kopie“.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Vytvářet záložní kopie">
                                                                        <longdesc>Jestli má gedit vytvářet záložní kopie souborů při jejich uložení. Ve volbě „Přípona záložní kopie“ můžete nastavit příponu těchto záložních souborů.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Schéma stylů">
                                                                        <longdesc>ID formátovacího schématu GtkSourceView používaného pro barvu textu.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Písmo editoru">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Používat standardní písmo">
                                                                        <longdesc>Má-li být pro editovaný text použito standardní písmo systému místo písma specifického pro aplikaci GEdit. Je-li tato volba vypnuta, bude místo systémového písma použito písmo z volby „Písmo editoru“.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
