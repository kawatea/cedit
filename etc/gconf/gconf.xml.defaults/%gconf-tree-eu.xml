<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Aktibatu pluginak">
                                                        <longdesc>Plugin aktiboen zerrenda. Plugin aktiboen &quot;Helbidea&quot; dauka. Ikusi .gedit-plugin fitxategia plugin zehatz baten kokalekua jakiteko.</longdesc>
                                                </local_schema>
                                        </entry>
                                        <dir name="checkupdate">
                                                <entry name="ignore_version">
                                                        <local_schema short_desc="Ezikusi egingo den bertsioa hurrengoa kaleratu arte">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="filebrowser">
                                                <entry name="filter_pattern">
                                                        <local_schema short_desc="Fitxategi-arakatzailearen iragazki-eredua">
                                                                <longdesc>Iragazki-eredua fitxategi-arakatzailearekin iragazteko. Iragazki honek iragazki-moduaren gainean funtzionatzen du.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="filter_mode">
                                                        <local_schema short_desc="Fitxategi-arakatzailearen iragazki modua">
                                                                <longdesc>Balio honek fitxategi-arakatzailetik iragaziko diren fitxategiak zehazten du. Balio posibleak: &apos;none&apos; (ez du iragazten), &apos;hidden&apos; (iragazi ezkutuko fitxategiak), &apos;binary&apos; (iragazi fitxategi bitarrak) eta &apos;hidden_and_binary&apos; (iragazi bai ezkutukoak bai bitarrak).</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <entry name="open_at_first_doc">
                                                        <local_schema short_desc="Ezarri aurreneko dokumentuaren kokalekua">
                                                                <longdesc>TRUE (egia) gisa ezartzen bada, fitxategi-arakatzaileak oraindik erabili ez duen irekitako aurreneko dokumentuaren direktorioa bistaratuko du fitxategi-arakatzailearen osagaiak. Kasu hau dokumentua komando lerrotik edo Nautilus-etik irekitzen denean gertatu ohi da.</longdesc>
                                                        </local_schema>
                                                </entry>
                                                <dir name="on_load">
                                                        <entry name="enable_remote">
                                                                <local_schema short_desc="Gaitu urruneko kokalekuak berreskuratzea">
                                                                        <longdesc>Urruneko kokalekuak berreskuratzea edo ez ezartzen du.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="virtual_root">
                                                                <local_schema short_desc="Fitxategi-arakatzailearen erro birtualeko direktorioa">
                                                                        <longdesc>Fitxategi-arakatzailearen erro birtualeko direktorioa, fitxategi-arakatzailearen osagaia kargatzean eta onload/tree_view EGIA denean erabiltzeko. Erro birtuala uneko erroari dagokio beti.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="root">
                                                                <local_schema short_desc="Fitxategi-arakatzailearen erroko direktorioa">
                                                                        <longdesc>Fitxategi-arakatzailearen erroko direktorioa, fitxategi-arakatzailearen osagaia kargatzean eta onload/tree_view EGIA denean erabiltzeko.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tree_view">
                                                                <local_schema short_desc="Ireki zuhaitz ikuspegiarekin">
                                                                        <longdesc>Ireki zuhaitz-ikuspegian fitxategi-arakatzailearen osagaiak kargatzen duenean laster-marken ikuspegiaren ordez</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Gaitu sintaxiaren nabarmentzea">
                                                                <longdesc>Gedit-ek sintaxiaren nabarmentzea gaitu behar duen ala ez.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Menuan agertzen diren kodeketak">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Automatikoki detektatutako kodeketak">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Inprimatzeko lerro-zenbakiaren letra-tipoa">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Inprimatzeko goiburuen letra-tipoa">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Testuaren letra-tipoa inprimatzeko">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Inprimatu lerro-zenbakiak">
                                                                        <longdesc>Balioa 0 bada, ez da lerro-zenbakirik txertatuko dokumentua inprimatzean. Bestalde, gedit-ek lerro-zenbakiak inprimatuko ditu adierazitako lerro-kopuru bakoitzeko.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Lerroaren itzulbiratze-modua inprimatzen">
                                                                        <longdesc>Lerro luzeak inprimatzeko nola itzulbiratu zehazten du. Erabili &quot;GTK_WRAP_NONE&quot; ez itzulbiratzeko, &quot;GTK_WRAP_WORD&quot; hitzaren mugetan itzulbiratzeko eta &quot;GTK_WRAP_CHAR&quot;  karaktere indibidualen mugetan itzulbiratzeko. Kontuan hartu balioek maiuskulak/minuskulak desberdintzen dituztela; beraz, ziurtatu hemen aipatu bezalaxe agertzen direla.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Inprimatu goiburua">
                                                                        <longdesc>Gedit-ek dokumentuak inprimatzean dokumentuaren goiburua sartu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Inprimatu sintaxiaren nabarmentzea">
                                                                        <longdesc>gedit-ek dokumentuak inprimatzean sintaxiaren nabarmentzea inprimatu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Fitxategi berrienen gehienezko kopurua">
                                                                        <longdesc>&quot;Fitxategi berrienak&quot; azpimenuan bistaratuko diren fitxategi ireki berrien gehienezko kopurua zehazten du.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Beheko panela ikusgai dago">
                                                                        <longdesc>Editatzeko leihoen beheko panelak ikusgai egon behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="side_pane">
                                                        <entry name="side_pane_visible">
                                                                <local_schema short_desc="Alboko panela ikusgai dago">
                                                                        <longdesc>Editatzeko leihoen alboko panelak ikusgai egon behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Egoera-barra ikusgai dago">
                                                                        <longdesc>Editatzeko leihoen beheko egoera-barrak ikusgai egon behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Tresna-barrako botoien estiloa">
                                                                        <longdesc>Tresna-barrako botoien estiloa. Balio posibleak hauek dira: &quot;GEDIT_TOOLBAR_SYSTEM&quot; sistemaren estilo lehenetsia erabiltzeko, &quot;GEDIT_TOOLBAR_ICONS&quot; ikonoak bakarrik bistaratzeko &quot;GEDIT_TOOLBAR_ICONS_AND_TEXT&quot; ikonoak eta testua bistaratzeko, eta &quot;GEDIT_TOOLBAR_ICONS_BOTH_HORIZ&quot; ikonoen ondoko testu lehenetsia bistaratzeko. Kontuan hartu balioek maiuskulak/minuskulak desberdintzen dituztela; beraz, ziurtatu hemen aipatu bezalaxe agertzen direla.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Tresna-barra ikusgai dago">
                                                                        <longdesc>Tresna-barrak editatzeko leihoetan ikusgai egon behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Gaitu bilaketaren nabarmentzea">
                                                                        <longdesc>Gedit-ek bilaketako testuaren gertaera guztiak nabarmendu behar dituen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="cursor_position">
                                                        <entry name="restore_cursor_position">
                                                                <local_schema short_desc="Leheneratu kurtsorearen aurreko posizioa">
                                                                        <longdesc>Fitxategi bat kargatzean Gedit-ek kurtsorearen aurreko posizioa leheneratu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="smart_home_end">
                                                        <entry name="smart_home_end">
                                                                <local_schema short_desc="Hasiera Amaiera azkarra">
                                                                        <longdesc>Kurtsorea nola mugitzen den zehazten du HASIERA eta AMAIERA teklak sakatzen direnean. Erabili &quot;DESGAITUTA&quot; beti lerroaren hasierara/amaierara mugitzeko. &quot;ONDOREN&quot;  lerroaren hasierara/amaierara mugitzeko teklak aurreneko aldiz sakatzen direnean eta testuaren hasierara/amaierara (zuriuneei ez ikusi eginez) mugitzeko teklak bigarren aldiz sakatzean. &quot;AURRETIK&quot; testuaren hasierara/amaierara mugitzeko lerroaren hasierara/amaierara mugitu aurretik. Edo erabili &quot;BETI&quot; testuaren hasierara/amaierara beti mugitzeko, lerroaren hasierara/amaierara mugitu ordez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Eskuineko marjinaren kokalekua">
                                                                        <longdesc>Eskuineko marjinaren kokalekua zehazten du.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Bistaratu eskuineko marjinak">
                                                                        <longdesc>Gedit-ek editatzeko arean eskuineko marjina bistaratu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Nabarmendu bat-datorren parentesia">
                                                                        <longdesc>Gedit-ek hautatutako parentesiari dagokion bikotea nabarmendu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Nabarmendu uneko lerroa">
                                                                        <longdesc>Gedit-ek uneko lerroa nabarmendu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Bistaratu lerro-zenbakiak">
                                                                        <longdesc>Gedit-ek editatzeko arean lerro-zenbakiak bistaratu behar dituen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Koska automatikoa">
                                                                        <longdesc>Gedit-ek koska automatikoa gaitu behar duen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Txertatu zuriuneak">
                                                                        <longdesc>Gedit-ek tabuladoreen ordez zuriuneak sartu behar dituen ala ez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Tabuladorearen tamaina">
                                                                        <longdesc>Tabuladore-karaktereen ordez bistaratu behar diren zuriune-kopurua zehazten du.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Lerroaren itzulbiratze-modua">
                                                                        <longdesc>Lerro luzeak editatzeko arean nola itzulbiratu zehazten du. Erabili &quot;GTK_WRAP_NONE&quot; ez itzulbiratzeko, &quot;GTK_WRAP_WORD&quot; hitzaren mugetan itzulbiratzeko eta &quot;GTK_WRAP_CHAR&quot; karaktere indibidualen mugetan itzulbiratzeko. Kontuan hartu balioek maiuskulak/minuskulak desberdintzen dituztela; beraz, ziurtatu hemen aipatu bezalaxe agertzen direla.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Gehiengo desegite-ekintza kopurua">
                                                                        <longdesc>Gedit-ek desegin edo berregin ditzakeen ekintzen gehienezko kopurua. Erabili &quot;-1&quot; ekintza kopurua ez mugatzeko.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="undo_actions_limit">
                                                                <local_schema short_desc="Desegin ekintzen muga (ZAHARKITUA)">
                                                                        <longdesc>Gedit-ek desegin edo berregin ditzakeen ekintzen gehienezko kopurua. Erabili &quot;-1&quot; ekintza kopurua ez mugatzeko.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="writable_vfs_schemes">
                                                                <local_schema short_desc="VFS eskema idazgarriak">
                                                                        <longdesc>Gedit-ek idazteko moduan onartzen dituen VFS eskemen zerrenda. &apos;fitxategia&apos; eskema idazgarria da lehenetsi gisa.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Automatikoki gordetzeko bitartea">
                                                                        <longdesc>Gedit-ek aldatutako fitxategiak automatikoki gorde aurretik zenbat minutu itxaron behar duen. Eragina izan dezan, &quot;Gorde automatikoki&quot; aukerak aktibatuta egon behar du.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Gorde automatikoki">
                                                                        <longdesc>Gedit-ek aldatutako fitxategiak denbora-tarte baten ondoren automatikoki gorde behar dituen ala ez. Denbora-tartea ezar dezakezu &quot;Automatikoki gordetzeko bitartea&quot; aukeraren bidez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Babeskopiaren luzapena">
                                                                        <longdesc>Babeskopien izenek erabiltzeko luzapena edo atzizkia. &quot;Sortu babeskopiak&quot; aukera aktibatuta dagoenean bakarrik izango du eragina</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Sortu babeskopiak">
                                                                        <longdesc>Gedit-ek gordetzen dituen fitxategien babeskopiak sortu behar dituen ala ez. Babeskopiaren luzapena ezar dezakezu &quot;Babeskopiaren luzapena&quot; aukeraren bidez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Estilo-eskema">
                                                                        <longdesc>Testuaren kolorean erabilitako GtkSourceView estilo-eskemaren IDa.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Editorearen letra-tipoa">
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Erabili letra-tipo lehenetsia">
                                                                        <longdesc>Testua editatzeko, gedit-en berezko letra-tipoaren ordez sistemaren letra-tipo lehenetsia erabiliko den ala ez. Aukera hori desaktibatuta badago, &quot;Editorearen letra-tipoa&quot; aukeran adierazitako letra-tipoa erabiliko da sistemaren letra-tipoaren ordez.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
