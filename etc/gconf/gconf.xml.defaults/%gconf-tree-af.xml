<?xml version="1.0"?>
<gconf>
        <dir name="schemas">
                <dir name="apps">
                        <dir name="gedit-2">
                                <dir name="plugins">
                                        <entry name="active-plugins">
                                                <local_schema short_desc="Aktiewe inproppe">
                                                </local_schema>
                                        </entry>
                                </dir>
                                <dir name="preferences">
                                        <dir name="syntax_highlighting">
                                                <entry name="enable">
                                                        <local_schema short_desc="Aktiveer sintaksverligting">
                                                                <longdesc>Of gedit sinsverligting moet aktiveer.</longdesc>
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="encodings">
                                                <entry name="shown_in_menu">
                                                        <local_schema short_desc="Enkoderings in kieslys vertoon">
                                                        </local_schema>
                                                </entry>
                                                <entry name="auto_detected">
                                                        <local_schema short_desc="Outobespeurde enkoderings">
                                                        </local_schema>
                                                </entry>
                                        </dir>
                                        <dir name="print">
                                                <dir name="fonts">
                                                        <entry name="print_font_numbers_pango">
                                                                <local_schema short_desc="Reëlnommerfont vir drukwerk">
                                                                        <longdesc>Spesifiseer watter font vir reëlnommers gebruik moet word by drukwerk. Dit sal slegs in werking tree wanneer die &quot;Druk reëlnommers&quot;-opsie nie-nul is.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_header_pango">
                                                                <local_schema short_desc="Kopfont vir drukwerk">
                                                                        <longdesc>Spesifiseer watter font vir bladsykoppe gebruik moet word by drukwerk. Dit sal eers in werking tree as die &quot;Druk kop&quot;-opsie in werking gestel word.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_font_body_pango">
                                                                <local_schema short_desc="Lyffont vir drukwerk">
                                                                        <longdesc>Spesifiseer watter font vir &apos;n dokument se lyf gebruik moet word wanneer dokumente gedruk word.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="page">
                                                        <entry name="print_line_numbers">
                                                                <local_schema short_desc="Druk reëlnommers">
                                                                        <longdesc>As hierdie waarde 0 is, sal geen reëlnommers ingevoeg word wanneer &apos;n dokument gedruk word nie. Andersins sal gedit reëlnommers by elke soveel reëls druk.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_wrap_mode">
                                                                <local_schema short_desc="Drukreëlvoumodus">
                                                                        <longdesc>Spesifiseer hoe lang reëls gevou word vir drukwerk. Gebruik &quot;GTK_WRAP_NONE&quot; vir geen omvou, &quot;GTK_WRAP_WORD&quot; vir omvou by woordgrense, en &quot;GTK_WRAP_CHAR&quot; vir omvou by individuele karakters se grense. Let op dat die waardes kas-sensitief is.  Maak dus seker hulle verskyn presies soos hierbo vermeld.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_header">
                                                                <local_schema short_desc="Druk kop">
                                                                        <longdesc>Of gedit &apos;n dokumentkop moet insluit wanneer dit dokumente druk.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="print_syntax_highlighting">
                                                                <local_schema short_desc="Druk sintaksverligting">
                                                                        <longdesc>Of gedit sinsverligting moet druk wanneer dit dokumente druk.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="ui">
                                                <dir name="recents">
                                                        <entry name="max_recents">
                                                                <local_schema short_desc="Maksimum onlangse lêers">
                                                                        <longdesc>Spesifiseer die maksimum getal onlangs geopende lêers wat in die &quot;Onlangse lêers&quot;-subkieslys vertoon sal word.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bottom_panel">
                                                        <entry name="bottom_panel_visible">
                                                                <local_schema short_desc="Onderste paneel is sigbaar">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="statusbar">
                                                        <entry name="statusbar_visible">
                                                                <local_schema short_desc="Statusstaaf is sigbaar">
                                                                        <longdesc>Of die statusstaaf onderaan redigeervensters sigbaar moet wees.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="toolbar">
                                                        <entry name="toolbar_buttons_style">
                                                                <local_schema short_desc="Nutsbalkknoppiestyl">
                                                                        <longdesc>Styl vir nutsbalkknoppies. Moontlike waardes is &quot;GEDIT_TOOLBAR_SYSTEM&quot; vir die stelsel se verstekstyl, &quot;GEDIT_TOOLBAR_ICONS&quot; om net ikone te vertoon, &quot;GEDIT_TOOLBAR_ICONS_AND_TEXT&quot; om beide ikone en teks te vertoon, en &quot;GEDIT_TOOLBAR_ICONS_BOTH_HORIZ&quot; om geprioritiseerde teks langs ikone te vertoon. Let op dat die waardes kas-sensitief is.  Maak dus seker hulle verskyn presies soos hierbo vermeld.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="toolbar_visible">
                                                                <local_schema short_desc="Nutsbalk is sigbaar">
                                                                        <longdesc>Of die nutsbalk in redigeervensters sigbaar moet wees.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                        <dir name="editor">
                                                <dir name="search_highlighting">
                                                        <entry name="enable">
                                                                <local_schema short_desc="Aktiveer verligting van soektogte">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="right_margin">
                                                        <entry name="right_margin_position">
                                                                <local_schema short_desc="Regterkantlyn-posisie">
                                                                        <longdesc>Spesifiseer die posisie van die regterkantlyn.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="display_right_margin">
                                                                <local_schema short_desc="Vertoon regterkantlyn">
                                                                        <longdesc>Of gedit die regterkantlyn in die redigeerbare streek moet vertoon.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="bracket_matching">
                                                        <entry name="bracket_matching">
                                                                <local_schema short_desc="Verlig passende hakkie">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="current_line">
                                                        <entry name="highlight_current_line">
                                                                <local_schema short_desc="Verlig huidige reël">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="line_numbers">
                                                        <entry name="display_line_numbers">
                                                                <local_schema short_desc="Vertoon reëlnommers">
                                                                        <longdesc>Of gedit reëlnommers in die redigeerbare streek moet vertoon.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="auto_indent">
                                                        <entry name="auto_indent">
                                                                <local_schema short_desc="Outokeep">
                                                                        <longdesc>Of gedit outo-inkeping moet aktiveer.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="tabs">
                                                        <entry name="insert_spaces">
                                                                <local_schema short_desc="Voeg spasies in">
                                                                        <longdesc>Of gedit spasies in plaas van keepkarakters moet invoeg.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="tabs_size">
                                                                <local_schema short_desc="Keepgrootte">
                                                                        <longdesc>Spesifiseer die getal spasies wat vertoon moet word in plaas van keepkarakters.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="wrap_mode">
                                                        <entry name="wrap_mode">
                                                                <local_schema short_desc="Reëlvoumodus">
                                                                        <longdesc>Spesifiseer hoe om lang reëls in die redigeerbare streek te vou. Gebruik &quot;GTK_WRAP_NONE&quot; vir geen omvou, &quot;GTK_WRAP_WORD&quot; vir omvou by woordgrense, en &quot;GTK_WRAP_CHAR&quot; vir omvou by individuele karakters se grense. Let op dat die waardes kas-sensitief is.  Maak dus seker hulle verskyn presies soos hierbo vermeld.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="undo">
                                                        <entry name="max_undo_actions">
                                                                <local_schema short_desc="Maksimum aantal ontdoen-aksies">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="save">
                                                        <entry name="auto_save_interval">
                                                                <local_schema short_desc="Outostoor-interval">
                                                                        <longdesc>Getal minute na gedit gewysigde lêers outomaties sal stoor. Dit sal eers in werking tree as die &quot;Outostoor&quot;-opsie in werking gestel word.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="auto_save">
                                                                <local_schema short_desc="Outostoor">
                                                                        <longdesc>Of gedit gewysigde lêers outomaties na &apos;n sekere interval moet stoor. Jy kan die interval met die &quot;Outostoorinterval&quot;-opsie stel.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="backup_copy_extension">
                                                                <local_schema short_desc="Rugsteunkopie-uitbreiding">
                                                                        <longdesc>Uitbreiding of agtervoegsel om vir rugsteunlêername te gebruik. Dit sal eers in werking tree as die &quot;Maak rugsteunkopieë&quot;-opsie in werking gestel word.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="create_backup_copy">
                                                                <local_schema short_desc="Maak rugsteunkopieë">
                                                                        <longdesc>Of gedit rugsteunkopieë moet maak van die lêers wat dit stoor. Jy kan die rugsteunlêer-uitbreiding met die &quot;Rugsteunkopie-uitbreiding&quot;-opsie stel.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="colors">
                                                        <entry name="scheme">
                                                                <local_schema short_desc="Stylskema">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                                <dir name="font">
                                                        <entry name="editor_font">
                                                                <local_schema short_desc="Redigeerfont">
                                                                        <longdesc>&apos;n Doelgemaakte font wat vir die redigeerbare streek gebruik sal word. Dit sal eers in werking tree as die &quot;Gebruik verstekfont&quot;-opsie buite werking gestel word.</longdesc>
                                                                </local_schema>
                                                        </entry>
                                                        <entry name="use_default_font">
                                                                <local_schema short_desc="Gebruik verstekfont">
                                                                </local_schema>
                                                        </entry>
                                                </dir>
                                        </dir>
                                </dir>
                        </dir>
                </dir>
        </dir>
</gconf>
